{
  "name": "sprucebot-cli",
  "version": "0.1.8",
  "description": "A command-line tool to manage sprucebot projects",
  "main": "sprucebot.js",
  "repository": "git@github.com:sprucelabsai/sprucebot-cli.git",
  "author": "Spruce Labs, inc",
  "license": "None",
  "preferGlobal": true,
  "bin": {
    "sprucebot": "./bin/sprucebot.js",
    "sprucebotProgram": "./bin/sprucebot-platform.js"
  },
  "scripts": {
    "start": "node ./bin/sprucebot.js",
    "lint": "./node_modules/.bin/standard",
    "test": "./node_modules/mocha/bin/_mocha ./test/**/**.js",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- ./test/**/**.js",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm run test"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "config": "^1.26.2",
    "hostile": "^1.3.0",
    "js-yaml": "^3.10.0",
    "nodegit": "^0.20.2",
    "yeoman-environment": "^2.0.3",
    "yeoman-generator": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "command-line-test": "^1.0.10",
    "husky": "^0.14.3",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^3.5.3",
    "sinon": "^3.2.1",
    "standard": "^10.0.3",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.7.0"
  }
}
