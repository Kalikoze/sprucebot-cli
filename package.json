{
  "name": "sprucebot-cli",
  "version": "0.1.0",
  "description": "A command-line tool to manage sprucebot projects",
  "main": "sprucebot.js",
  "repository": "git@github.com:sprucelabsai/sprucebot-cli.git",
  "author": "Spruce Labs, inc",
  "license": "None",
  "preferGlobal": true,
  "bin": {
    "sprucebot": "./bin/sprucebot.js",
    "sprucebotProgram": "./bin/sprucebot-platform.js"
  },
  "scripts": {
    "start": "node ./bin/sprucebot.js",
    "lint": "./node_modules/.bin/standard",
    "test": "./node_modules/.bin/_mocha -R spec ./test/** --recursive",
    "cover": "istanbul cover --no-default-excludes --hook-run-in-context -x \"node_modules/**\" ./node_modules/.bin/_mocha -R spec ./test/** --recursive",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm run test"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "config": "^1.26.2",
    "yeoman-environment": "^2.0.2",
    "yeoman-generator": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "command-line-test": "^1.0.10",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "standard": "^10.0.3"
  }
}
